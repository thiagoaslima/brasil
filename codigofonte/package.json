    "dev:build": "webpack --config webpack.config.js --watch",
    "dev:serve": "webpack-dev-server --content-base wwwroot/ --history-api-fallback",
    "start": "concurrently \"npm run dev:build\" \"npm run dev:serve\"",
    "start:prerender": "concurrently \"npm run dev:build\" \"dotnet run\"",
    "build": "npm run version && webpack  --config webpack.config.vendor.js && webpack --config webpack.config.js",
