  "scripts": {
    "test": "node --inspect node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch",
    "old:prestart": "webpack --config webpack.config.vendor.js && webpack --config webpack.config.js --watch",
    "old:start": "webpack-dev-server --content-base wwwroot/ --history-api-fallback",
    "old:build": "npm run version && webpack --config webpack.config.vendor.js && webpack --config webpack.config.js",
