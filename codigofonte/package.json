  "jest": {
    "globals": {
      "__TS_CONFIG__": "<rootDir>/tsconfig.json"
    },
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/ClientApp/setupJest.ts",
    "moduleNameMapper": {
      "app/(.*)": "<rootDir>/ClientApp/app/$1"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "ClientApp/**/*.{ts}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/*.{d.ts}"
    ],
    "transform": {
      "^.+\\.(ts|js|html)$": "<rootDir>/node_modules/jest-preset-angular/preprocessor.js",
      "\\.(properties)$": "<rootDir>/jestTransform.js"
    },
    "coverageDirectory": "coverage"
  }
